<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="DuckestDungeon" namespace="TestProgram">
    <class name="Product">
        <id name="Id">
            <generator class="native" />
        </id>
        <property name="Name" />
        <property name="Category" />
        <property name="Discontinued" />
        <many-to-one name="Price" class="Price" />
        <bag name="Stores" table="Product_Store" >
            <key column="ProductID" />
            <many-to-many class="TestProgram.Store" column="StoreID" />
        </bag>
    </class>
  
</hibernate-mapping>

